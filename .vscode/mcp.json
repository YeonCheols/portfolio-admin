{
    "inputs": [
      {
        "type": "promptString",
        "id": "api-key",
        "description": "API Key",
        "password": true
      }
    ],
    "servers": {
        "github": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-github"],
        "env": {
          "GITHUB_PERSONAL_ACCESS_TOKEN": "${input:api-key}"
        }
      }
    }
  }
  